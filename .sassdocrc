autofill:
  - throw
  - content
  - require
exclude:
  - ./css/*
  - ./stylesheets/vendor/*
groups:
  components: CSS components
  config: Configuration variables and maps
  font-stacks: Font-stacks
  qa: Quality assurance
  hacks: Dirty Hacks & Fixes
  helpers: Helpers
  normalizer: Normalization
herman:
  customCSS: './testResults/index.css'
  sass:
    includepaths:
      - 'stylesheets/abstracts'
    includes:
      - 'constants'
      - 'font-stacks'
      - 'functions'
      - 'variables'
      - 'mixins'
package:
  title: Initial CSS
  name: initial-css
  version: 0.1
  license: your project’s license
  homepage: URL to your project’s homepage
  description: css boilerplate and collection of helpers for SASS and CSS
strict: true
theme: herman
verbose: true
